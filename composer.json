{
    "name": "etsglobal/php-static-analysis",
    "description": "Static analysis tools configuration for PHP projects",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "jangregor/phpstan-prophecy": "^1.0",
        "pdepend/pdepend": "^2.14",
        "phpmd/phpmd": "^2.12",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^9.6",
        "sebastian/phpcpd": "^6.0.3",
        "slevomat/coding-standard": "^8.12",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "ETSGlobalCodingStandard\\": "src\\ETSGlobalCodingStandard\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ETSGlobalCodingStandard\\": "tests\\ETSGlobalCodingStandard\\"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
