{
    "name": "etsglobal/php-static-analysis",
    "description": "Static analysis tools configuration for PHP projects",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "jangregor/phpstan-prophecy": "^1.0",
        "phpmd/phpmd": "^2.15",
        "phpspec/prophecy-phpunit": "^2.1",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.6 || ^10.0 || ^11.0",
        "slevomat/coding-standard": "^8.14",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "ETSGlobalCodingStandard\\": "src\\ETSGlobalCodingStandard\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ETSGlobalCodingStandard\\": "tests\\ETSGlobalCodingStandard\\"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
