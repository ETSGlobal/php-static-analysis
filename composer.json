{
    "name": "etsglobal/php-static-analysis",
    "description": "Static analysis tools configuration for PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.9",
        "phpstan/phpstan": "^0.12.56",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpmd/phpmd": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "ETSGlobalCodingStandard\\": "src\\ETSGlobalCodingStandard\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ETSGlobalCodingStandard\\": "tests\\ETSGlobalCodingStandard\\"
        }
    },
    "minimum-stability": "stable"
}
